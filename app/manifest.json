{
    "manifest_version": 2,
    "name": "Youtube Not Gif",
    "description": "This extension will marks all youtube links as [YOUTUBE] on reddit",
    "version": "0.0.1",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "options_ui": {
	"page": "options.html",
	"chrome_style": true
	},
    "permissions": [ "storage" ],
    "content_scripts": [
        {
            "matches": [
               "*://*.reddit.com/*"
            ],
            "js": [
                "contentscript.js"
            ],
            "run_at": "document_end"
        }
    ]

}
