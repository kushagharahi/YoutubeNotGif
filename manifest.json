{
    "manifest_version": 2,
    "name": "Youtube Not Gif",
    "description": "This extension will marks all youtube links as [YOUTUBE] on reddit",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://reddit.com/*"
            ],
            "js": [
                "script.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "tabs"
    ]
}